    "import pandas as pd\n",
    "\n",
    "def load_csv(file_path):\n",
    "    \"\"\"\n",
    "    Loads a CSV file and returns a DataFrame.\n",
    "    \"\"\"\n",
    "    try:\n",
    "        data = pd.read_csv(file_path)\n",
    "        return data\n",
    "    except Exception as e:\n",
    "        print(f\"Error loading CSV: {e}\")\n",
    "        return None\n"
